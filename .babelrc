{
  "presets": [
    "es2015",
    "react",
    "stage-0",
    ["env", {
    "targets": {
      "browsers": [
        "Chrome >= 52",
        "FireFox >= 44",
        "Safari >= 7",
        "Explorer 11",
        "last 4 Edge versions"
      ]
    },
    "useBuiltIns": true
  }]
  ],
  "plugins": [
    ["transform-react-jsx", { "pragma": "h" }]
  ],
}
